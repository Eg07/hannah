    "version": "0.2.0",
    "configurations": [



        {
            "name": "standard",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=3",
                "model=tc-res8"
            ]
        },
        {
            "name": "kd",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "config_kd",
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=3",
                "model=tc-res4"
            ]
        },
        {
            "name": "ofa",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "model=ofa_group_test_small",
                "nas= ",
                "module.num_workers=8"
            ]
        },
        {
            "name": "ofa limited",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=1",
                "model=ofa_group_test",
                "nas=ofa_nas_group_test",
                "module.num_workers=8"
            ]
        },
        {
            "name": "ofa limited small",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=1",
                "model=ofa_group_test_small",
                "nas=ofa_nas_group_test",
                "module.num_workers=8"
            ]
        },
        {
            "name": "object_detection",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "justMyCode": false,
            "args": [
                "--config-name",
                "config_object_detection",
                "dataset.data_folder=/local/datasets"
            ]
        },
    ]
}
